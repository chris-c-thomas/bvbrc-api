{
  "name": "p3api",
  "version": "2.0.3",
  "private": true,
  "scripts": {
    "start": "node --unhandled-rejections=strict --stack-size=65536 app.js",
    "local": "node app.js",
    "dev": "nodemon --exec 'npm run swagger && node app.js'",
    "docs:swagger-js": "node swagger.js",
    "docs:export": "node scripts/export-merged-swagger.js",
    "docs:serve": "node server.js",
    "docs:pdf": "node scripts/export-docs-pdf.js",
    "docs:export:solr": "node openapi-solr.js",
    "docs:scalar": "npx scalar reference swagger-output.json --port 3000",
    "docs:sdk:ts": "openapi-generator generate -i swagger-output.json -g typescript-axios -o sdk/typescript-axios",
    "docs:sdk:py": "openapi-generator generate -i swagger-output.json -g python -o sdk/python",
    "docs:sdk:go": "openapi-generator generate -i swagger-output.json -g go -o sdk/go",
    "docs:sdk:java": "openapi-generator generate -i swagger-output.json -g java -o sdk/java",
    "docs:sdk:cs": "openapi-generator generate -i swagger-output.json -g csharp -o sdk/csharp",
    "docs:sdk:all": "npm run docs:sdk:ts && npm run docs:sdk:py && npm run docs:sdk:go && npm run docs:sdk:java && npm run docs:sdk:cs",
    "docs:publish": "cp docs/index.html /path/to/your/site && cp swagger-output-flat.* /api/",
    "docs:publish:gh-pages": "mkdir -p gh-pages && cp docs/index.html swagger-output-flat.* gh-pages/ && npx gh-pages -d gh-pages",
    "test-api": "mocha -R spec tests/test-api/test.*.spec.js",
    "test-rpc": "mocha -R spec tests/test-rpc/test.*.spec.js",
    "test-media": "mocha -R spec tests/test-media/test.*.spec.js",
    "test-jbrowse": "mocha spec tests/test-jbrowse/test.spec.js",
    "test-hpi": "mocha spec tests/test-hpi/test.spec.js",
    "test-content": "mocha spec tests/test-content/test.spec.js",
    "test-download": "mocha spec tests/test-download/test.spec.js",
    "test-permissions": "mocha -R spec  \"tests/test-permissions/**/*.spec.js\"",
    "build-css": "npx postcss public/css/docs.css -o public/css/compiled.css",
    "build-image": "./buildImage.sh"
  },
  "dependencies": {
    "apicache": "^1.6.2",
    "archiver": "^7.0.0",
    "async-transforms": "^1.0.9",
    "axios": "^1.8.2",
    "body-parser": "^1.20.3",
    "cookie-parser": "~1.4.7",
    "cors": "~2.5.0",
    "debug": "^3.2.7",
    "ejs": "^3.1.10",
    "event-stream": "^3.3.2",
    "express": "^4.21.2",
    "file-queue": "^0.2.1",
    "forever": "^4.0.3",
    "formidable": "^1.2.2",
    "fs-extra": "~1.0.0",
    "glob": "^11.0.1",
    "js-yaml": "^4.1.0",
    "json-patch": "^0.6.1",
    "JSONStream": "^1.3.1",
    "lucene-query-parser": "^1.2.0",
    "md5": "^2.1.0",
    "morgan": "^1.9.1",
    "nconf": "^0.11.4",
    "node-xlsx": "^0.24.0",
    "p3-user": "git+https://github.com/PATRIC3/p3_user.git#c0fee2a0ac833fa78401266d705f8c5a6b80e4f3",
    "pino": "^6.2.1",
    "postcss-cli": "^11.0.1",
    "recast": "^0.23.11",
    "redis": "^3.1.1",
    "request-promise": "4.2.2",
    "rql": "^0.3.3",
    "solrjs": "github:BV-BRC-dependencies/solrjs",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "temp": "^0.8.3",
    "touch": "^1.0.0",
    "uuid": "^2.0.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@redocly/cli": "^1.34.0",
    "@scalar/cli": "^0.2.325",
    "autoprefixer": "^10.3.7",
    "chai": "4.1.2",
    "eslint": "^7.8.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^7.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^4.0.0",
    "mocha": "^7.2.0",
    "nodemon": "^3.1.9",
    "pm2": "^4.4.0",
    "pm2-logrotate": "^2.7.0",
    "postcss": "^8.3.10",
    "puppeteer": "^24.4.0",
    "swagger-autogen": "^2.23.7",
    "tailwindcss": "^2.2.17"
  }
}
